package Lab4_n01415959;
import java.util.Random;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		//Get Name, ContactNumber and NumberOfPackages from Customer(User)
		String customerName;
		int numberOfPackages;
		long contactNumber;
		double discountRate = 0.0;
		int purchase = 1250;
		double percent = 0.54;
		boolean valid = true;
		
		int limit = 0;
		for (int i = 10; i >= limit; i -= 2)
		{
		    for (int j = i; j <= 1; j++)
		    {
		        System.out.println("In inner for loop");
		    }
		    System.out.println("In outer for loop");
		}
		

		System.exit(0);
	}

}

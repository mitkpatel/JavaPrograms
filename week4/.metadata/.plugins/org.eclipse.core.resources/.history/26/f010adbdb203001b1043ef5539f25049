package Week4Lab3;

public class Temperature {
		
		private double celsius, fahrenheit, kalvin;
	
		public void setCelsiusTemperature(double newCelsius) {
			celsius = newCelsius;
		}
		
		public double getFahrenheit() {
			
			fahrenheit = (temperature * 1.8) + 32;
			return fahrenheit;
		}

		public double kalvinConvert(double temperature) {
			
			kalvin = (temperature +  273.15);
			return kalvin;
		}
}
